{
  "name": "homepage",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest --environment jsdom",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --no-fix --ignore-path .gitignore && stylelint \"**/*.vue\" \"**/*.scss\""
  },
  "dependencies": {
    "@vueuse/core": "^8.7.5",
    "pinia": "^2.0.13",
    "vue": "^3.2.37",
    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "@iconify-icons/bi": "^1.2.5",
    "@iconify-icons/line-md": "^1.2.9",
    "@iconify-icons/material-symbols": "^1.2.7",
    "@iconify/vue": "^3.2.1",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/consola": "^2.2.5",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^18.0.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.1",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.27.0",
    "eslint-plugin-vue": "^8.2.0",
    "jsdom": "^20.0.0",
    "sass": "^1.53.0",
    "stylelint": "^14.9.1",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^4.0.0",
    "typescript": "~4.7.4",
    "vite": "^2.9.13",
    "vite-plugin-checker": "^0.4.6",
    "vue-tsc": "^0.38.2",
    "vitest": "^0.16.0"
  },
  "volta": {
    "node": "16.15.1"
  }
}
